;
; BIND data file for local loopback interface
;
$TTL	604800
@	IN	SOA	localhost. root.localhost. (
			      2		; Serial
			 604800		; Refresh
			  86400		; Retry
			2419200		; Expire
			 604800 )	; Negative Cache TTL
;
@	IN	NS	localhost.
@	IN	A	192.169.6.65
sk	IN	A	192.169.6.66
server	IN	A	192.0.0.0
coredns	IN	A	192.0.10.0
coretcp	IN	A	192.0.10.66
dnsdist	IN	A	192.0.20.0
